{
  "name": "arena",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn buildsrc:arena && yarn buildplace:arena",
    "buildsrc:arena": "rbxtsc",
    "buildplace:arena": "mkdir -p builds && rojo build --output builds/arena.rbxlx arena.project.json",
    "watch:arena": "rbxtsc -w",
    "serve:arena": "rojo serve arena.project.json",
    "dev:arena": "yarn build && yarn shortcircuitdev:arena",
    "shortcircuitdev:arena": "concurrently -k 'yarn watch:arena' 'yarn serve:arena'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rbxts/compiler-types": "^2.0.2-types.0",
    "@rbxts/types": "^1.0.642",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "concurrently": "^8.0.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-roblox-ts": "^0.0.30",
    "prettier": "^2.3.2",
    "rbxts-transformer-flamework": "^1.0.0-beta.21",
    "roblox-ts": "^2.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@flamework/components": "^1.0.0-beta.21",
    "@flamework/core": "^1.0.0-beta.21",
    "@flamework/networking": "^1.0.0-beta.21",
    "assets": "1.0.0"
  }
}
