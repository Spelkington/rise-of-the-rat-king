{
  "name": "arena",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn compile && mkdir -p builds && rojo build --output builds/arena.rbxlx arena.project.json",
    "compile": "rbxtsc",
    "watch": "rbxtsc -w",
    "serve": "rojo serve arena.project.json",
    "dev": "yarn build && yarn shortcircuitdev:arena",
    "connect-io": "npx io-serve",
    "shortcircuitdev:arena": "concurrently -k 'yarn watch' 'yarn serve' 'yarn connect-io'"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^2.1.1-types.0",
    "@rbxts/types": "^1.0.677",
    "cgc-eslint-config": "workspace:^",
    "roblox-ts": "^2.1.0"
  },
  "dependencies": {
    "@rbxts/knit": "^0.0.21-alpha.0",
    "@rbxts/services": "^1.5.1",
    "assets": "1.0.0",
    "concurrently": "^8.0.1"
  }
}
