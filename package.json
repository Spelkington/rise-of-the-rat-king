  "workspaces": [
    "workspaces/*",
    "workspaces/**",
    "cgc-eslint-config"
  ],
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "build": "yarn prebuild && yarn workspaces foreach -pt run build",
